{
  "name": "@redactie/revisions-module",
  "version": "0.1.4",
  "description": "redactie revisions module",
  "main": "dist/redactie-revisions-module.umd.js",
  "module": "dist/redactie-revisions-module.umd.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/redactie-revisions-module.umd.js",
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:types && webpack --env.prod",
    "build:w": "npm run build:types && webpack -w --env.prod",
    "build:analyse": "npm run build:types && webpack --env.analyse",
    "start": "concurrently --kill-others \"npm run build:w\" \"cd demo && npm run start\"",
    "lint:scss": "stylelint public/**/*.scss --allow-empty-input",
    "lint:ts": "eslint --cache --cache-location .eslintcache --ext .ts,.tsx public",
    "lint": "npm run lint:scss && npm run lint:ts",
    "test": "jest ./public"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.antwerpen.be/projects/REDA/repos/redactie-revisions_module_react"
  },
  "keywords": [
    "Redactie",
    "React",
    "Revisions",
    "Module"
  ],
  "publishConfig": {
    "@redactie:registry": "https://nexusrepo.antwerpen.be/repository/npm-private"
  },
  "devDependencies": {
    "@acpaas-ui/react-components": "^6.0.6",
    "@acpaas-ui/react-editorial-components": "0.0.16",
