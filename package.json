    "@redactie/translations-module": "^1.1.0",
    "@redactie/utils": "2.0.1",
    "@redactie/workflows-module": "0.2.5",
    "@testing-library/jest-dom": "5.0.2",
    "@testing-library/react": "9.4.0",
    "@types/classnames": "^2.3.1",
    "@types/jest": "24.9.1",
